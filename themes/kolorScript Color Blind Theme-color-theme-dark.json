{
	"name": "kolorScript Color Blind Theme",
	"colors": {
		"editor.background": "#1a1a1a",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Invalids",
			"scope": [				
				"invalid"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Constants",
			"scope": [				
				"constant"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#277bb3"
			}
		},
		{
			"name": "Variables",
			"scope": [				
				"variable"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#cc00ff"
			}
		},
		{
			"name": "Defines",
			"scope": [				
				"define"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#e74c3c"
			}
		},
		{
			"name": "Compiles",
			"scope": [				
				"compile"
			],
			"settings": {
				"foreground": "#2ECC71"
			}
		},		
		{
			"name": "Interprets",
			"scope": [
				"interpret"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f1c40f"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#bbbbbb"
			}
		}		
	]
}
